{
    "env": {
        "browser": true,
        "es6": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
    ],
    "globals": {
      "convertNumber": false,
      "formatComma": false,
      "formatCommaOrEmpty": false,
      "formatDate": false,
      "changeReadonly": false,
      "parseIntOrZero": false,
      "parseFloatOrDefault": false,
      "nullToBlank": false,
      "nashi": false,
      "zenkaku2Hankaku":false,
      "setMyPartnerCd": false,
      "isInvalidAmount": false
    },
    "rules": {
      "semi": "error",
      "arrow-body-style": "error",
      "arrow-parens": "error",
      "arrow-spacing": "error",
      "generator-star-spacing": "error",
      "no-duplicate-imports": "error",
      "no-useless-computed-key": "error",
      "no-useless-constructor": "error",
      "no-useless-rename": "error",
      "prefer-const": "error",
      "prefer-rest-params": "error",
      "prefer-spread": "error",
      "rest-spread-spacing": "error",
      "template-curly-spacing": "error",
      "yield-star-spacing": "error"
    }
}
